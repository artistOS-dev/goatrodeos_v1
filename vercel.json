{
  "version": 2,
  "buildCommand": "npm run build -w client",
  "outputDirectory": "client/dist",
  "installCommand": "npm install",
  "routes": [
    {
      "src": "^/api/(.*)",
      "status": 404
    },
    {
      "src": "^/(?!api)(.*)$",
      "dest": "/index.html"
    }
  ],
  "env": {
    "VITE_API_URL": "@vite_api_url"
  }
}
